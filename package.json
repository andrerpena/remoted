    "sitemap": "^2.1.0",
    "slack": "^11.0.2",
    "slugify": "^1.3.4"
  },
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "node .production-server/server/index.js",
    "lint": "prettier --write \"**/*.ts\"",
    "graphqlgen": "graphqlgen",
    "db:dump": "cross-env COMMAND=dump_db ts-node --project tsconfig.server.json ./tools/db-manager.ts",
    "db:restore": "cross-env COMMAND=restore_db ts-node --project tsconfig.server.json ./tools/db-manager.ts",
    "db:drop": "cross-env COMMAND=drop_dev_dbs ts-node --project tsconfig.server.json ./tools/db-manager.ts",
    "db:install": "cross-env COMMAND=create_dev_dbs ts-node --project tsconfig.server.json ./tools/db-manager.ts",
    "db:reinstall": "npm run db:drop && npm run db:install",
    "tools:fake": "cross-env NODE_ENV=development ts-node --project tsconfig.server.json ./tools/fake.ts",
    "test": "jest --config=jest.unit.config.js --detectOpenHandles",
    "generate": "gql-gen --config codegen.yml",
    "playground": "ts-node --project tsconfig.server.json ./playground.ts",
    "fix-shit": "ts-node --project tsconfig.server.json ./fix-shit.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/classnames": "^2.2.7",
    "@types/colors": "^1.2.1",
    "@types/dotenv": "^6.1.0",
    "@types/email-templates": "^3.5.1",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.2",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.11",
    "@types/massive": "^5.4.0",
    "@types/mime-types": "^2.1.0",
    "@types/next": "^8.0.3",
    "@types/next-seo": "^1.10.0",
    "@types/react": "^16.8.8",
    "@types/react-autosuggest": "^9.3.7",
    "@types/react-select": "^2.0.15",
    "@types/recharts": "^1.1.16",
    "@types/sharp": "^0.22.1",
    "@types/showdown": "^1.9.2",
    "cross-env": "^5.2.0",
    "graphql-code-generator": "^0.16.1",
    "graphql-codegen-introspection": "^0.16.1",
    "graphql-codegen-typescript-client": "^0.16.1",
    "graphql-codegen-typescript-common": "^0.16.1",
    "graphql-codegen-typescript-resolvers": "^0.16.1",
    "graphql-codegen-typescript-server": "^0.16.1",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "jest-cli": "^24.1.0",
    "nodemon": "^1.18.7",
    "npm": "^6.9.0",
    "pg-monitor": "^1.1.0",
    "prettier": "1.15.3",
    "pretty-quick": "^1.8.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1",
    "webpack": "^4.28.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
