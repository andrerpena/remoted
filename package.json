    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.3",
    "isomorphic-unfetch": "^3.0.0",
    "massive": "^5.6.0",
    "mime-types": "^2.1.22",
    "next": "latest",
    "next-seo": "^1.11.2",
    "node-sass": "^7.0.0",
    "pug": "^2.0.3",
